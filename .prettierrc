{
	"$schema": "https://json.schemastore.org/prettierrc",
	"arrowParens": "avoid",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"printWidth": 120,
	"quoteProps": "as-needed",
	"semi": true,
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": false
}
